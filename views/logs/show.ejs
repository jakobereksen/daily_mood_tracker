<table class="table">
  <tr>
    <th>Factor of wellbeing</th>
    <td>
      <%= logs.factorOfWellbeing %>
    </td>
  </tr>
  <tr>
    <th>Description</th>
    <td>
      <%= logs.annotation%>
    </td>
  </tr>
  <tr>
    <th>Date</th>
    <td>
      <%= logs.referenceDate%>
    </td>
  </tr>
</table>

<div>
  <a class="button" href="<%=`/logs/${logs._id}/edit`%>">
    Edit Course Details
  </a>
</div>
<div>
  <a class="button" href="<%= `/logs/${logs._id}/delete?_method=DELETE` %>" onclick="return confirm('Are you sure you want to delete this record?')">Delete</a>
</div>
